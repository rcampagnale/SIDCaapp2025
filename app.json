{
  "expo": {
    "name": "SiDCa",
    "slug": "sidca-tu-sindicato",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logos/logo-01.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/logos/logo-01.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff",
      "permissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE"
      ]
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/apk.jpeg",
        "resizeMode": "contain"
      },
      "package": "com.jakiro12.one"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "386928ec-45b3-4bb5-9710-6e61d994263a"
      },
      "router": {
        "origin": false
      }
    },
    "owner": "rcampagnale"
  }
}
